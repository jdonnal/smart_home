<!doctype html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="assets/css/bootstrap.min.css">
    <!-- Font Awesome CSS -->
    <link href="assets/css/all.min.css" rel="stylesheet">

    <!-- load the page specif CSS -->
    {% block css %}{% endblock %}
    <title>{% block title %} Default Title {% endblock %}</title>
</head>
<body>
    <div class="container-fluid">
        <!-- page specific content here -->
        {% block content %}
            Default Content
        {% endblock %}
    </div>

    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="assets/js/jquery-3.3.1.min.js"></script>
    <script src="assets/js/popper.min.js"></script>
    <script src="assets/js/bootstrap.bundle.min.js"></script>
    <script src="assets/js/jquery.canvaswrapper.js"></script>
    <script src="assets/js/jquery.colorhelpers.js"></script>
    <script src="assets/js/jquery.flot.js"></script>
    <script src="assets/js/jquery.flot.saturated.js"></script>
    <script src="assets/js/jquery.flot.browser.js"></script>
    <script src="assets/js/jquery.flot.drawSeries.js"></script>
    <script src="assets/js/jquery.flot.uiConstants.js"></script>
    <script src="assets/js/jquery.flot.time.js"></script>
    <script src="assets/js/jquery.flot.resize.js"></script>


    <!-- load the page specific js -->
    {% block js %}{% endblock %}
    </body>
</html>